  .addField("<a:Tick:714477040312647700> ❤️ Meu criador"," ▸ <@!613726577758306306>") //exemplo: <@!613726577758306306>
  .addField("<a:Tick:714477040312647700> 📅 Dia que o bot foi criado", " ▸ 06/01/2020")
  .addField("<a:Tick:714477040312647700> 🛠️ Linguagem"," ▸ discord.js ")
